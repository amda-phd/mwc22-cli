{
  "name": "mwc22-cli",
  "version": "0.5.0",
  "description": "Access all the MWC22 participants' information from your shell",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "report:todo": "rm -rf todo.md && leasot --exit-nicely --reporter markdown \"./**/*.js\" --ignore \"node_modules/**/*.js\" > todo.md && code -r todo.md",
    "test": "jest",
    "test:watch": "jest --watch --runInBand --detectOpenHandles --coverage=false --verbose=false"
  },
  "bin": {
    "mwc22": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amda-phd/hackaton-4YFN.git"
  },
  "keywords": [
    "backend",
    "cli",
    "commander",
    "mongoDB"
  ],
  "author": "amda",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amda-phd/hackaton-4YFN#issues"
  },
  "homepage": "https://github.com/amda-phd/hackaton-4YFN#readme",
  "devDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.1.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "leasot": "^12.0.0",
    "nodemon": "^2.0.15",
    "pretty-quick": "^3.1.3"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^6.2.1",
    "validator": "^13.7.0"
  },
  "_moduleAliases": {
    "Bin": "bin",
    "Func": "lib/functions",
    "Models": "lib/models",
    "@app": "lib/app.js",
    "@cli": "lib/interfaces/cli.js",
    "@config": "config.json",
    "@index": "bin/index.js",
    "@mongo": "lib/db/mongo.js",
    "@pack": "package.json"
  },
  "nodemonConfig": {
    "exec": "npm install -g .",
    "ignore": [
      "tests/"
    ]
  }
}
